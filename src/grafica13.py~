#!encoding:UTF-8
#!/usr/bin/python

import pylab as pl

x1=[0.00001,0.000031,0.000041,0.000056,0.000067]
x2=[10,110,200,230,320]
y1=[40,25,80,80,100]
y2=[20,30,30,70,90]
y3=[10,10,20,50,70]
y4=[5,20,30,30,60]
y5=[0.000256,0.000126,0.000120,0.000119,0.000117]


pl.subplot (211)
pl.plot(x1,y1,label='10')
pl.plot(x1,y2,color='r',label='110')
pl.plot(x1,y3,color='m',label='200')
pl.plot(x1,y4,color='g',label='320')
pl.legend()

pl.subplot (212)
pl.plot(x2,y5)
pl.legend()

pl.show()